    "dependency-check": "^2.5.1",
    "eslint": "^1.6.0",
    "istanbul": "^0.3.22",
    "istanbul-coveralls": "^1.0.3",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.1"
